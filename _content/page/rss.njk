---

permalink: "feed.xml"
eleventyExcludeFromCollections: true

---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">
  <title>Posts on Josh Can Help</title>
  <subtitle>Notes on technology, programming, parenting, and self improvement.</subtitle>
  <link href="https://www.joshcanhelp.com/feed.xml" rel="self"/>
  <link href="https://www.joshcanhelp.com"/>
  <id>https://www.joshcanhelp.com</id>
  <updated>{{ collections.rssCollection | rssLastUpdatedDate }}</updated>
  <author>
    <name>Josh Cunningham</name>
    <email>josh@joshcanhelp.com</email>
  </author>
  {%- for post in collections.rssCollection.slice(0, 9) %}
    {% set absolutePostUrl %}{{ post.url | url | absoluteUrl("https://www.joshcanhelp.com") }}{% endset %}
    <entry>
      <title>{{ post.data.title }}</title>
      <link href="{{ absolutePostUrl }}"/>
      <updated>{{ post.date | rssDate }}</updated>
      <id>{{ absolutePostUrl }}</id>
      <content type="html">{{ post.templateContent | htmlToAbsoluteUrls(absolutePostUrl) }}</content>
    </entry>
  {%- endfor %}
</feed>